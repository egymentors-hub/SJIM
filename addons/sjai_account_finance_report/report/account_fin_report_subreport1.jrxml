	<queryString>
		<![CDATA[SELECT
    d.id as parent_id, d.code as parent_code, d.name as parent_name, SUBSTRING(d.code, 1, 1) AS char1_parent_code, d.parent_id AS parent_parent_id,
    c.id as child1_id, c.code as child1_code, c.name as child1_name, SUBSTRING(c.code, 1, 1) AS char1_child1_code, c.parent_id AS child1_parent_id,
    b.id as child2_id, b.code as child2_code, b.name as child2_name, SUBSTRING(b.code, 1, 1) AS char1_child2_code, b.parent_id AS child2_parent_id,
    a.id as child3_id, a.code as child3_code, a.name as child3_name, SUBSTRING(a.code, 1, 1) AS char1_child3_code, a.parent_id AS child3_parent_id,
    (SELECT SUM(COALESCE(ml.balance, 0.0))
     FROM account_move_line AS ml LEFT JOIN account_move AS m ON ml.move_id=m.id
     WHERE (ml.account_id IN (SELECT id FROM account_account WHERE parent_id = a.id
         OR parent_id IN (SELECT id FROM account_account WHERE parent_id = a.id
           OR parent_id IN (SELECT id FROM account_account WHERE parent_id = a.id
	    OR parent_id IN (SELECT id FROM account_account WHERE parent_id = a.id))))
             OR ml.account_id = a.id)
         AND $P!{state_query} AND ml.date <= $P{current_month}) AS current_month,
    (SELECT SUM(COALESCE(ml.balance, 0.0))
     FROM account_move_line AS ml LEFT JOIN account_move AS m ON ml.move_id=m.id
     WHERE (ml.account_id IN (SELECT id FROM account_account WHERE parent_id = a.id
         OR parent_id IN (SELECT id FROM account_account WHERE parent_id = a.id
           OR parent_id IN (SELECT id FROM account_account WHERE parent_id = a.id
	    OR parent_id IN (SELECT id FROM account_account WHERE parent_id = a.id))))
             OR ml.account_id = a.id)
         AND $P!{state_query} AND ml.date <= $P{prev_month}) AS prev_month

FROM account_account as a
	full outer join account_account as b on a.parent_id=b.id
	full outer join account_account as c on b.parent_id=c.id
	full outer join account_account as d on c.parent_id=d.id

WHERE true

ORDER BY d.code, c.code, b.code, a.code ASC]]>
