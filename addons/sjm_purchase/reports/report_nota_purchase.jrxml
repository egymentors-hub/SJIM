	<queryString language="SQL">
		<![CDATA[SELECT 1 AS order_no
, NULL AS gen_number
, pol.id AS line_id
, company.id AS company_id
, company.name AS company_name
, company.partner_office_id AS company_head_office
, po.service_order AS product_type
, po.name AS purchase_name
, po.date_order AS purchase_date
, po.date_planned AS date_planned
, po.partner_ref AS reference
, (select string_agg(pr.name,',') 
   from purchase_order_purchase_request_rel poprl 
	 LEFT JOIN purchase_request pr ON pr.id = poprl.purchase_request_id 
	 where poprl.purchase_order_id = po.id) as purchase_request
, apt.name AS payment_name
, si.name AS incoterm_name
, vendor.name AS vendor_name
, vendor.street AS vendor_street
, vendor.street2 AS vendor_street2
, vendor.city AS vendor_city
, vendor_state.name AS vendor_state
, vendor.vat AS vendor_vat
, vendor.phone AS vendor_phone
, vendor_attn.name AS vendor_attn_name
, vendor_attn.phone AS vendor_attn_phone
, shipping.name AS shipping_name
, shipping.street AS shipping_street
, shipping.street2 AS shipping_street2
, shipping.city AS shipping_city
, shipping.vat AS shipping_vat
, shipping.phone AS shipping_phone
, shipping.mobile AS shipping_mobile
, shipping_parent.name AS shipping_parent_name
, invoice.name AS invoice_name
, invoice.street AS invoice_street
, invoice.street2 AS invoice_street2
, invoice.city AS invoice_city
, invoice.vat AS invoice_vat
, invoice.phone AS invoice_phone
, invoice.mobile AS invoice_mobile
, invoice.fax AS invoice_fax
, invoice_parent.name AS invoice_parent_name
, pt.default_code AS product_code
, pt.name AS product_name
, (CASE WHEN pol.name ilike '['||pt.default_code||'] '||pt.name THEN NULL ELSE pol.name END) AS line_description
, pol.product_qty AS qty
, pu.name AS uom_name
, pol.price_unit AS price_unit
, pol.price_subtotal AS price_subtotal
, floor(po.amount_untaxed) AS amount_untaxed
, 0 AS amount_discount
, floor(po.amount_tax) AS amount_tax
, floor(po.amount_total) AS amount_total
, po.notes AS notes
, curr.name AS currency_name
, curr.symbol AS currency_symbol
, po.report_sign_1 AS report_sign_1
, po.report_sign_2 AS report_sign_2
, po.report_sign_3 AS report_sign_3
, po.report_sign_4 AS report_sign_4
, po.report_sign_5 AS report_sign_5
, po.report_sign_6 AS report_sign_6
, res_doc.header_report AS header_model
, CASE WHEN res_doc.transport THEN 'transport'
	WHEN res_doc.spk THEN 'spk'
	ELSE 'purchase' END AS type_document
FROM purchase_order_line pol
INNER JOIN purchase_order po ON po.id = pol.order_id
LEFT OUTER JOIN res_company company ON company.id = po.company_id
LEFT OUTER JOIN account_payment_term apt ON apt.id = po.payment_term_id
LEFT OUTER JOIN stock_incoterms si ON si.id = po.incoterm_id
LEFT OUTER JOIN res_partner vendor ON vendor.id = po.partner_id
LEFT OUTER JOIN res_country_state vendor_state ON vendor_state.id = vendor.state_id
LEFT OUTER JOIN res_partner vendor_attn ON vendor_attn.id = (SELECT id FROM res_partner where parent_id = po.partner_id and type = 'contact' limit 1)
LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
LEFT OUTER JOIN res_partner shipping_parent ON shipping_parent.id = shipping.parent_id
LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
LEFT OUTER JOIN res_partner invoice_parent ON invoice_parent.id = invoice.parent_id
LEFT OUTER JOIN product_product pp ON pp.id = pol.product_id
LEFT OUTER JOIN product_uom pu ON pu.id = pol.product_uom
LEFT OUTER JOIN product_template pt ON pt."id" = pp.product_tmpl_id
LEFT OUTER JOIN res_currency curr ON curr."id" = po.currency_id
LEFT OUTER JOIN res_document_type res_doc ON res_doc.id = po.doc_type_id
WHERE po.id = $P{id} 
ORDER BY order_no, line_id]]>
